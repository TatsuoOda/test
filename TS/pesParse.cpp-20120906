	    if( bitrate_info_present_flag==1 )
	    {
		avg_bitrate                      = GetBitStream( fp, 16 );
		max_bitrate_layer                = GetBitStream( fp, 16 );
		max_bitrate_layer_representation = GetBitStream( fp, 16 );
		max_bitrate_calc_window          = GetBitStream( fp, 16 );
	    }
