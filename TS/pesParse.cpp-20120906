int sub_region_layer_flag = (-1);
int iroi_division_info_present_flag = (-1);
int profile_level_info_present_flag = (-1);
int bitrate_info_present_flag = (-1);
int frm_rate_info_present_flag = (-1);
int frm_size_info_present_flag = (-1);
